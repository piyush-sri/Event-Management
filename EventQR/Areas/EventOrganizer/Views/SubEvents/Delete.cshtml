@model EventQR.Models.SubEvent

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_OrganizerLayout.cshtml";
    ViewBag.activeTabName = "thisEventSubEvents";

}@await Html.PartialAsync("_eventPartial")




<div class="row">
   
    <div class="col-sm-12">
        <div class="content">
            <div class="card">
                <div class="card-header">
                    <h4 style="text-align:center"><b>Do You want to delete sub event?</b></h4>
                    <div class="modal-body">
                        <dl class="row">
                          
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.SubEventName)
                            </dt>
                            <dd class="col-sm-8">
                                @Html.DisplayFor(model => model.SubEventName)
                            </dd>
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.StartDateTime)
                            </dt>
                            <dd class="col-sm-8">
                                @Html.DisplayFor(model => model.StartDateTime)
                            </dd>
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.EndDateTime)
                            </dt>
                            <dd class="col-sm-8">
                                @Html.DisplayFor(model => model.EndDateTime)
                            </dd>
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.CreatedDate)
                            </dt>
                            <dd class="col-sm-8">
                                @Html.DisplayFor(model => model.CreatedDate)
                            </dd>
                            
                        </dl>

                        <form asp-action="Delete">
                            <input type="hidden" asp-for="UniqueId" />
                            <input type="submit" value="Delete" class="btn btn-danger" /> |
                            <a asp-action="Index">Back to List</a>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
    
</div>

