@model EventQR.Models.TicketScanner

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_OrganizerLayout.cshtml";
    ViewBag.activeTabName = "thisEventScanner";

}
@await Html.PartialAsync("_eventPartial")

<div class="row">
    
    <div class="col-sm-12">
        <div class="content">
            <div class="card">
                <div class="card-header">
                    <h4 style="text-align:center"><b>Ticket Scanner Details</b></h4>
                    <hr/>
                    <div class="modal-body">
                        <dl class="row">
                          
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.EmailId)
                            </dt>
                            <dd class="col-sm-8">
                                @Html.DisplayFor(model => model.EmailId)
                            </dd>
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.Password)
                            </dt>
                            <dd class="col-sm-8">
                                @Html.DisplayFor(model => model.Password)
                            </dd>
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.ConfirmPassword)
                            </dt>
                            <dd class="col-sm-8">
                                @Html.DisplayFor(model => model.ConfirmPassword)
                            </dd>
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.CreatedData)
                            </dt>
                            <dd class="col-sm-8">
                                @Model.CreatedData.ToString("dd/MMM/yyyy h:mm tt")
                            </dd>
                            <dt class="col-sm-4">
                                @Html.DisplayNameFor(model => model.LastUpdatedData)
                            </dt>
                            <dd class="col-sm-8">
                                @Model.LastUpdatedData.ToString("dd/MMM/yyyy h:mm tt")
                            </dd>
                        </dl>
                    </div>
                    <div>
                        <a asp-action="Create" asp-route-id="@Model?.UniqueId">Edit</a> |
                        <a asp-action="Index">Back to List</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
  
</div>

