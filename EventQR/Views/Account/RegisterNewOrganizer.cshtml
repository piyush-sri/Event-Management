<body class="hold-transition login-page">
    <div class="login-box"> </div>
    <div class="container">
        <div class="card">
            <div class="card-header text-center bg-dark text-white text-bold">Register As Event Manager Organization</div>
            <div class="card-body">
                <form class="form-horizontal" asp-action="RegisterNewOrganizer" asp-controller="Account" method="post">
                    <div class="row form-group">
                        <label class="control-label col-sm-2" asp-for="OrganizationName"></label>
                        <div class="col-sm-10">
                            <input type="text" asp-for="OrganizationName" placeholder="Event Manager Organization Name" class="form-control" />
                            <span asp-validation-for="OrganizationName" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row form-group">
                        <label class="control-label col-sm-2" asp-for="PhoneNumber"></label>
                        <div class="col-sm-10">
                            <input type="number" asp-for="PhoneNumber" placeholder="Primary Contact Number" class="form-control" />
                            <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row form-group">
                        <label class="control-label col-sm-2" asp-for="Email"></label>
                        <div class="col-sm-10">
                            <input asp-for="Email" class="form-control" placeholder="Registration mail Id (used for login)" />
                            <span asp-validation-for="Email" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row form-group">
                        <label class="control-label col-sm-2" asp-for="Password"></label>
                        <div class="col-sm-10">
                            <input asp-for="Password" class="form-control" type="password" placeholder="Enter password" />
                            <span asp-validation-for="Password" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row form-group">
                        <label class="control-label col-sm-2" asp-for="ConfirmPassword"></label>
                        <div class="col-sm-10">
                            <input asp-for="ConfirmPassword" class="form-control" type="password" placeholder="Re Enter password" />
                            <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-sm-2"> </div>
                        <div class="col-sm-10">
                            <button type="submit" class="btn btn-primary">SignUp </button>
                            <div asp-validation-summary="All" class="text-primary"></div>
                        </div>
                    </div>

                </form>
            </div>
            <div class="card-footer text-center  "> Already have an account?  &nbsp;  &nbsp;  &nbsp; <a asp-controller="Account" class="btn btn-primary" asp-action="Login">Login </a>  </div>
        </div>
        <main role="main" class="pb-3">
            <div class="row">
                <div class="col-md-4">
                    <section>
                    </section>
                </div>
            </div>
        </main>
    </div>
    <script src="~/uitemplate/admintemp/dist/js/jquery.min.js"></script>
    <script src="~/uitemplate/admintemp/dist/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/uitemplate/admintemp/dist/js/niche.html"></script>
</body>

</html>






