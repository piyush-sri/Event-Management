@inject SignInManager<AppUser> signInManager;
@model AppUser
@{
    ViewData["Title"] = "Register";
    Layout = null;// "~/Views/Shared/_LandingPageLayout.cshtml";
    ViewBag.ActiveTabId = 4;
}
@section Scripts{
    <script src="~/AppJs/Address.js"></script>
}

<!DOCTYPE html>
<html lang="en">

